# Load Data
stocks <- read.csv("C:/Users/natha/OneDrive/Desktop/personal/Data/closing_stocks_unpivoted.csv")
colors <- brewer.pal(4, "Set3")
# Create a user interface
ui <- fluidPage(
titlePanel("Interactive Stock Data"),
sidebarLayout(
sidebarPanel(
sliderInput(
inputId = "year",
label = "Year",
min = 2000,
max = 2020,
value = c(2000, 2021),
sep = ""),
checkboxGroupInput(
inputId = "stockID",
label = "Stock ID",
choices = c("MSFT", "AAPL", "GOOG", "AMZN", "IBM"),
selected = c("MSFT", "AAPL", "GOOG", "AMZN", "IBM"))),
mainPanel(
plotOutput(
outputId = "plot"))))
# Create a server
server <- function(input, output) {
output$plot <- renderPlot({
subset <- stocks %>%
as.data.frame()
plot(
x = stocks$Ã¯..Date,
y = stocks$StockPrice,
col = colors[as.integer(stocks$StockName)],
pch = 19,
cex = 1.5,
xlim = as.Date(c("2010-01-01","2019-12-31")),
ylim = c(0, 800),
xlab = "Date",
ylab = "Stock Price")
legend(
x = "topleft",
as.character(levels(stocks$StockName)),
col = colors[1:4],
pch = 19,
cex = 1.5)})
}
# Create a shiny app
shinyApp(
ui = ui,
server = server)
